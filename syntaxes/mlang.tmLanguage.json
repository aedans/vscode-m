{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "M Language",
	"patterns": [{ "include": "#expression" }],
	"repository": {
		"expression": {
			"patterns": [
				{
					"include": "#list-expression"
				},
				{
					"include": "#symbol-expression"
				},
				{
					"include": "#symbol-literal-expression"
				},
				{
					"include": "#comment"
				}
			]
		},
		"list-expression": {
			"begin": "(\\()([^ \t\r\n\";\\(\\)]*)",
			"end": "\\)",
			"beginCaptures": {
				"1": { "name": "punctuation.bracket.round" },
				"2": { "name": "keyword.mlang" }
			},
			"endCaptures": {
				"0": { "name": "punctuation.bracket.round" }
			},
			"name": "expression.group",
			"patterns": [{ "include": "#expression" }]
		},
		"symbol-expression": {
			"name": "variable.other.mlang",
			"patterns": [{
				"name": "variable.other.mlang",
				"match": "[^ \t\r\n\";\\(\\)]+"
			}]
		},
		"symbol-literal-expression": {
			"name": "string.quoted.mlang",
			"begin": "\"",
			"end": "\"",
			"patterns": [{
				"name": "string.quoted.mlang",
				"match": "[^\"]|\"\""
			}]
		},
		"comment": {
			"name": "comment.line.mlang",
			"begin": ";",
			"end": "\r|\n",
			"patterns": []
		}
	},
	"scopeName": "source.m"
}